{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**"]
    },
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": ["build", "^test"]
    },
    "dev": {
      "cache": false,
      "dependsOn": ["^dev"]
    }
  }
}
